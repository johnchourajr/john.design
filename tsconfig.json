{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@data/*": [
        "_data/*"
      ],
      "@lib/*": [
        "lib/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@globals/*": [
        "src/components/globals/*"
      ],
      "@style/*": [
        "src/components/style/*"
      ]
    },
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/pages/index.jsx"
  ],
  "exclude": [
    "node_modules"
  ]
}